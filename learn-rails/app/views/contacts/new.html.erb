<% content_for :title do %>Contact<% end %>
<%= form_for (@contact) do |f| %>
    <%= f.text_field :name, placeholder: "Name goes here" %><br><br>
    <%= f.email_field :email, placeholder: "Enter email" %><br><br>
    <%= f.text_field :content, placeholder: "Enter your content here..." %><br><br>
    <%= f.submit "Save Details" %>
<% end %>
